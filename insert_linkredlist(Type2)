class Node:
    def __init__(self,data):
        self.data=data
        self.next=None
        
def takeInput():
    arr=list(int(i) for i in input().split())
    head=None
    tail=None
    for ele in arr:
        if ele==-1:
            break
        newNode=Node(ele)
        if head is None:
            head=newNode
            tail=newNode
        else:
            tail.next=newNode
            tail=newNode
    return head

def printll(head):
    while head is not None:
        print(str(head.data)+"--->")
        head=head.next
    print("None")
    return 
head=takeInput()
printll(head)
